name: Splateds_Elementals
main: info.nothingspecial.Splateds_Elementals.Splateds_Elementals
version: B.1.2
authors: [Splated, awesomefacecreeper]
softdepend: [BarAPI]

commands:
   live:
      description: Awakens an Elemental.
      usage: Unknown Command <command> 
      permission: Elemental.Live
      permission-message: You don't have permission
      
   cmd:
      description: Commands an Elemental to do your bidding
      usage: Unknown Command <command> 
      permission: Elemental.cmd
      permission-message: You don't have permission

permissions:
    Elemental.Live:
        description: Awakens an Elemental
        default: op
       
